
public class Renkler {
	private String[] renkler=new String[] { "yavru ağzı", "cam göbeği", "van dyke kahverengisi",
			"hint sarısı", "alizarin kırmızısı", "kadmiyum sarısı",
			"gece yarısı siyahı", "prusya mavisi", "titanyum beyazı",
			"koyu siena" };

	public Renkler(String[] renkler) {
		this.setRenkler(renkler);
	}

	/**
	 * @return the renkler
	 */
	public String[] getRenkler() {
		return renkler;
	}

	/**
	 * @param renkler the renkler to set
	 */
	public void setRenkler(String[] renkler) {
		this.renkler = renkler;
	}
}